fo\"></div>\r\n    </div>\r\n</div>","components/common/social-links":"{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                {{#if display_name '===' 'google+'}}\r\n                    <a class=\"icon icon--google-plus\" href=\"{{url}}\" target=\"_blank\">\r\n                        <svg><use xlink:href=\"#icon-google-plus-circle\" /></svg>\r\n                    </a>\r\n                {{else}}\r\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\r\n                        <svg><use xlink:href=\"#icon-{{display_name}}-circle\" /></svg>\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n","components/common/subscription-form":"<h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h5>\r\n<!-- <p>{{!lang 'newsletter.subscribe_intro'}}</p> -->\r\n\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                <input class=\"button button--white form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n            </div>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n"}